{
  "name": "boids",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "webpack -w",
    "watch_go_boid_stuff_go"     : "GOOS=js GOARCH=wasm nodemon --watch './go_boid_stuff/*.go' --signal SIGTERM -e 'go' --exec 'go     build -C ./go_boid_stuff/ -o ../dist/boid.wasm'",
    "watch_go_boid_stuff_tiny_go": "GOOS=js GOARCH=wasm nodemon --watch './go_boid_stuff/*.go' --signal SIGTERM -e 'go' --exec 'tinygo build -C ./go_boid_stuff/ -o ../dist/boid.wasm'"
  },
  "author": "DiscRoomFan36",
  "license": "MIT",
  "devDependencies": {
    "ts-loader": "9.5.2",
    "typescript": "5.8.3",
    "webpack": "5.99.9",
    "webpack-cli": "6.0.1",
    "@types/offscreencanvas": "2019.7.3",
    "nodemon": "3.1.11"
  }
}
